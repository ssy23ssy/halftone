<div class="container">
  <style>
    @import url(http://fonts.googleapis.com/earlyaccess/hanna.css);
    .tstyle {
      font-family: 'Hanna';
      font-size: 30px;
      text-align: left;
    }
  </style>


  <p class="tstyle"><%= link_to "홈으로", root_path %></p>
  <%= form_for :travel, url: travel_index_path, method: :post do |f| %>
      <div class="form-group">
        <%= f.label :title %>
        <%= f.text_field :title, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :country %>
        <%= f.text_field :country, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :period %>
        <%= f.text_field :period, class: "form-control" %>
     </div>
      <div class="form-group">
        <%= f.label :food %>
        <%= f.text_field :food, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :sleep %>
        <%= f.text_field :sleep, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :trans %>
        <%= f.text_field :trans, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :free %>
        <%= f.text_field :free, class: "form-control" %>
      </div>
      <%= f.submit "제출", class: "btn btn-primary" %>
  <% end %>


  <p class="tstyle"><%= link_to "logout", destroy_user_session_path, method: :delete %>
    <%= link_to "show", '/travel/mylist' %></p>
</div>